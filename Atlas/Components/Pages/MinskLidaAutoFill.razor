    <script>
        let carouselDates = new Swiper('#nf-dates', {
                loop: false,
                speed: 700,
                navigation: {
                    nextEl: '.swiper-button-next',
                    prevEl: '.swiper-button-prev',
                }
                ,
                freeMode: true,
                spaceBetween: 0,
                slidesPerView: 3,
                slidesPerGroup: 1,
                initialSlide: 3,
                centeredSlides: true,
                breakpoints: {
                    768:
                        {
                            slidesPerView: 5,
                        }
                }
            })
        ;
        $('.js_change_date').on('click', function () {
            let self = $(this), index = self.data('index'), date = self.data('date');
            carouselDates.slideTo(index);
            self.addClass('is-active').parent().siblings().find('.is-active').removeClass('is-active');
            $('.js_date-pick[name="date"]').datepicker('setDate', date);
            $(".js_reservation-button").click();
        })</script>
